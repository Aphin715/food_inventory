<h1>Listing food_inventories</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Description</th>
      <th>Quantity</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @food_inventory.each do |food_inventory| %>
      <tr>
        <td><%= food_inventory.food %></td>
        <td><%= food_inventory.description %></td>
        <td><%= food_inventory.quantity %></td>
        <td><%= link_to 'Show', food_inventory %></td>
        <td><%= link_to 'Edit', edit_food_inventory_path(food_inventory) %></td>
        <td><%= link_to 'Destroy', food_inventory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food inventory', new_food_inventory_path %>